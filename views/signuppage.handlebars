    <div class="container-2">
        
        <form class="form" id="createAccount">
             <div class="logo"><i class="fa-thin fa-c signuplogo"></i>ode-Net</div>
             
            <h1 class="form__title">Create Account</h1>
            <div class="button-group">
            <button class="avatar" data-avatar="0"><img class="pic-pro" src="/images/avatar.jpg" alt="avatar0"/></button>
            <button class="avatar" data-avatar="1"><img class="pic-pro" src="/images/avatar1.png" alt="avatar1"/></button>
            <button class="avatar" data-avatar="2"><img class="pic-pro" src="/images/avatar2.png" alt="avatar2"/></button>
            <button class="avatar" data-avatar="3"><img class="pic-pro" src="/images/avatar3.png" alt="avatar3"/></button>
            <button class="avatar" data-avatar="4"><img class="pic-pro" src="/images/avatar4.png" alt="avatar4"/></button>
            <button class="avatar" data-avatar="5"><img class="pic-pro" src="/images/avatar5.png" alt="avatar5"/></button>
            <button class="avatar" data-avatar="6"><img class="pic-pro" src="/images/avatar6.png" alt="avatar6"/></button>
            </div>
            {{!-- <div> TO DO Choose Avatar </div> --}}
            <div class="form__message form__message--error"></div>
            <div class="form__input-group">
                <input type="text" class="form__input"  placeholder="First Name">
                <div class="form__input-error-message"></div>
            </div>
            <div class="form__input-group">
                <input type="text" class="form__input" placeholder="Last Name">
                <div class="form__input-error-message"></div>
            </div>
            <div class="form__input-group">
                <input type="text" class="form__input" placeholder="Github URL">
                <div class="form__input-error-message"></div>
            </div>
            <div class="form__input-group">
                <input type="text" class="form__input" placeholder="Bio: Tell Us About You">
                <div class="form__input-error-message"></div>
            </div>
            <div class="form__input-group">
                <input type="text" id="signupUsername" class="form__input" placeholder="Username">
                <div class="form__input-error-message"></div>
            </div>
            <div class="form__input-group">
                <input type="password" class="form__input" placeholder="Password">
                <div class="form__input-error-message"></div>
            </div>
            <div class="form__input-group">
                <input type="password" class="form__input"  placeholder="Confirm password">
                <div class="form__input-error-message"></div>
            </div>
            <button class="form__button" type="submit">Continue</button>
            <p class="form__text">
                <a class="form__link" href="./" id="linkLogin">Already have an account? Sign in</a>
            </p>
        </form>
    </div>
<script src="/js/profilePic.js"></script>
